{
	"name": "wealthcare-portal",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"start:ci": "ng serve --host 0.0.0.0 --port 4200",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"test:ci": "ng test --no-watch --no-progress --browsers=Headless",
		"lintcheck": "prettier src --check",
		"serve:ssr:WealthcarePortal": "node dist/wealthcare-portal/server/server.mjs",
		"e2e": "ng e2e",
		"cypress:open": "cypress open",
		"cypress:run": "cypress run --browser firefox",
		"cypress:ci": "start-server-and-test start:ci http://localhost:4200 cypress:run",
		"build:github": "ng build --configuration production-github",
		"deploy:github": "npm run build:github && npx angular-cli-ghpages --dir=dist/browser --no-silent"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^18.2.0",
		"@angular/cdk": "^18.2.8",
		"@angular/common": "^18.2.0",
		"@angular/compiler": "^18.2.0",
		"@angular/core": "^18.2.0",
		"@angular/forms": "^18.2.0",
		"@angular/material": "^18.2.8",
		"@angular/platform-browser": "^18.2.0",
		"@angular/platform-browser-dynamic": "^18.2.0",
		"@angular/platform-server": "^18.2.0",
		"@angular/router": "^18.2.0",
		"@angular/ssr": "^18.2.8",
		"@fortawesome/angular-fontawesome": "^0.15.0",
		"@fortawesome/free-solid-svg-icons": "^6.6.0",
		"@swimlane/ngx-charts": "^20.5.0",
		"chai": "^5.1.1",
		"d3": "^7.9.0",
		"express": "^4.18.2",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0",
		"wealthcare-portal": "file:",
		"zone.js": "~0.14.10"
	},
	"devDependencies": {
		"@angular-builders/custom-webpack": "^18.0.0",
		"@angular-devkit/build-angular": "^18.2.12",
		"@angular/cli": "^18.2.8",
		"@angular/compiler-cli": "^18.2.0",
		"@cypress/angular": "^2.1.0",
		"@cypress/schematic": "^2.5.2",
		"@types/cypress": "^1.1.6",
		"@types/d3-shape": "^3.1.6",
		"@types/express": "^4.17.17",
		"@types/jasmine": "~5.1.0",
		"@types/jest": "^29.5.13",
		"@types/mocha": "^10.0.9",
		"@types/node": "^18.19.64",
		"angular-cli-ghpages": "^2.0.3",
		"chai": "*",
		"cypress": "^13.15.0",
		"dotenv-webpack": "^8.1.0",
		"jasmine-core": "~5.2.0",
		"karma": "~6.4.0",
		"karma-chrome-launcher": "~3.2.0",
		"karma-coverage": "~2.2.0",
		"karma-jasmine": "~5.1.0",
		"karma-jasmine-html-reporter": "~2.1.0",
		"mocha": "^10.7.3",
		"prettier": "3.3.3",
		"start-server-and-test": "^2.0.8",
		"typescript": "~5.5.2"
	}
}
